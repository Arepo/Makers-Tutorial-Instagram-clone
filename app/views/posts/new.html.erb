<%= form_for @post, html: {multipart: true} do |f| %>

	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :description %>
	<%= f.text_area :description %>

	<%= f.label :tag_names, 'Tags' %>
	<%= f.text_field :tag_names, data: {role: 'tagsinput'} %>

	<%= f.label :picture %>
	<%= f.file_field :picture %>

	<%= f.label :address %>
	<%= f.text_field :address %>

	<%= f.submit 'Post' %>

<% end %>